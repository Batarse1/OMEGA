    <sql>SELECT
    D.NAME AS JOBTITLE,
    D.GROUP_NAME AS GROUPNAME,
    EPH.RATE,
    COUNT(*) AS QUANTITY,
	TRUNC(SYSDATE) AS LOADING_DATE
FROM
    ODS.EMPLOYEE E
JOIN
    ODS.EMPLOYEE_DEPARTMENT_HISTORY EDH ON E.BUSINESS_ENTITY_ID = EDH.BUSINESS_ENTITY_ID
JOIN
    ODS.DEPARTMENT D ON D.DEPARTMENT_ID = EDH.DEPARTMENT_ID
JOIN
    ODS.EMPLOYEE_PAY_HISTORY EPH ON E.BUSINESS_ENTITY_ID = EPH.BUSINESS_ENTITY_ID
GROUP BY
    D.NAME,
    D.GROUP_NAME,
    EPH.RATE</sql>
